  "info": {
    "author": "Thomas Liebetraut",
    "author_email": "thomas@tommie-lie.de",
    "bugtrack_url": null,
    "classifiers": [
      "Environment :: Console",
      "Intended Audience :: Developers",
      "Intended Audience :: System Administrators",
