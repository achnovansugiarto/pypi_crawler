  "info": {
    "author": "tasuren",
    "author_email": "tasuren@aol.com",
    "bugtrack_url": null,
    "classifiers": [
      "Programming Language :: Python :: 3.10",
      "Topic :: Text Processing :: Markup :: HTML",
