{
  "info": {
    "author": "Sneha Sunil, Jakob Thoms, Ritisha Sharma, Raul Aguilar",
    "author_email": "",
    "bugtrack_url": null,
    "classifiers": [
      "License :: OSI Approved :: MIT License",
      "Programming Language :: Python :: 3",
      "Programming Language :: Python :: 3.10",
