{
  "info": {
    "author": "Kristian Bonnici",
    "author_email": "kristiandaaniel@gmail.com",
    "bugtrack_url": null,
    "classifiers": [
      "Development Status :: 2 - Pre-Alpha",
      "Intended Audience :: Developers",
      "License :: OSI Approved :: MIT License",
