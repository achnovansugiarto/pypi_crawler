{
  "info": {
    "author": "Camptocamp,Akretion,Sodexis,Odoo Community Association (OCA)",
    "author_email": "support@odoo-community.org",
    "bugtrack_url": null,
    "classifiers": [
      "Framework :: Odoo",
      "Framework :: Odoo :: 12.0",
