{
  "info": {
    "author": "Yelyzaveta Velizhanina and Bernard Knaepen",
    "author_email": "velizhaninae@gmail.com",
    "bugtrack_url": null,
    "classifiers": [
      "Development Status :: 3 - Alpha",
