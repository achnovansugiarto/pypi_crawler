  "info": {
    "author": "Jordan Casoli, Nico Van Den Hooff, Arlin Cherian and Zheren Xu",
    "author_email": "",
    "bugtrack_url": null,
    "classifiers": [
      "License :: OSI Approved :: MIT License",
