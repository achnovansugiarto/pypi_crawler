{
  "info": {
    "author": "Alejandro Molina, Quentin Delfosse, Patrick Schramowski",
    "author_email": "molina@cs.tu-darmstadt.de, quentin.delfosse@cs.tu-darmstadt.de",
    "bugtrack_url": null,
    "classifiers": [
